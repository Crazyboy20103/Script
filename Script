local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()



function  Script()
    local Window = Library.CreateLib("YTScript", "Synapse")



    local Comabat = Window:NewTab("Combat")
    local CombatSection = combat:NewSection("Aim assist")
    
    ComabatSection:NewToggle("ToggleText", "ToggleInfo", function(state)
        if state then
            print("Toggle On")
        else
            print("Toggle Off")
        end
    end)


end
